typedef union {
    float f;
    unsigned short int i[2];
} S;
int main (int ac, char *av[]) {
    /* must be volatile to avoid long registers */
    volatile float tol = 1.0f;
    volatile float power = 1.0f;
    int n = 23;
    S f;
    while (1.0f + (tol * 0.5f) != 1.0f) {
	tol *= 0.5f;
    }
    while (n-- > 0) {
	power *= 0.5f;
    }
    if (tol != power) {
	return 1;
    }
    /* make sure 0 == -0 */
    tol = 0.0f; power = -tol;
    if (tol != power) {
	return 2;
    }
    /* make sure -0 is implemented */
    f.f = power;
    n = (f.i[0] != 0) ? 0 : 1;
    if (f.i[n] != 0x8000) {
	return 3;
    }
    return 0;
}

