# enable everything



# BC_ARG_ENABLE 1:[everything] 2:[almost-everything] 3:[Turn on compilation of almost everything] 4:[auto]
bc_everything=auto
bc_everything_default=auto
bc_everything_set=no
# Check whether --enable-almost-everything or --disable-almost-everything was given.
if test "${enable_almost_everything+set}" = set; then
  enableval="$enable_almost_everything"

	bc_everything_set=yes
	case "x$enableval" in
		x[yY][eE][sS])
			bc_everything=yes
			;;
		x[nN][oO])
			bc_everything=no
			;;
		x)
			bc_everything=yes
			;;
		x*)
			{ echo "$as_me:$LINENO: *** WARNING *** WARNING *** WARNING *** WARNING *** WARNING ***" >&5
echo "$as_me: *** WARNING *** WARNING *** WARNING *** WARNING *** WARNING ***" >&6;}
			{ echo "$as_me:$LINENO: Unexpected value of $enableval to --enable-almost-everything (expecting yes/no)" >&5
echo "$as_me: Unexpected value of $enableval to --enable-almost-everything (expecting yes/no)" >&6;}
			bc_everything="$enableval"
			;;
	esac


