# automatically enable and load our configure cache file if available

echo "$as_me:$LINENO: checking whether a configure cache exists" >&5
echo $ECHO_N "checking whether a configure cache exists... $ECHO_C" >&6
if test "x$cache_file" = "x/dev/null" ; then
	configure_cache=config.cache.${host_os}.${ac_hostname}
	CONFIG_CACHE=""
	if test -f "$configure_cache"; then
		if test "x`cat $configure_cache | grep ac_cv_env_CC_value`" != "xac_cv_env_CC_value=$CC" ; then
												echo "$as_me:$LINENO: result: found but compiler differs" >&5
echo "${ECHO_T}found but compiler differs" >&6
			rm -f "$configure_cache"
		elif test "x`cat $configure_cache | grep ac_cv_env_CPPFLAGS_value`" != "xac_cv_env_CPPFLAGS_value=$CPPFLAGS" ; then
												echo "$as_me:$LINENO: result: found but preprocessor flags differ" >&5
echo "${ECHO_T}found but preprocessor flags differ" >&6
			rm -f "$configure_cache"
		else
												last_modified="`ls -Lt $configure_cache configure`"
			case "x$last_modified" in
				xconfigure*)
					echo "$as_me:$LINENO: result: found but out of date" >&5
echo "${ECHO_T}found but out of date" >&6
					rm -f $configure_cache
					;;
			esac
		fi

				if test -f "$configure_cache" ; then
						echo "$as_me:$LINENO: result: found $configure_cache" >&5
echo "${ECHO_T}found $configure_cache" >&6
			case $configure_cache in
				\\/* | ?:\\/* )
					. $configure_cache
					;;
				*)
					. ./$configure_cache
					;;
			esac
		fi
	else
		echo "$as_me:$LINENO: result: not found" >&5
echo "${ECHO_T}not found" >&6
	fi

		if test "x$host_os" != "xirix6.5" ; then
		{ echo "$as_me:$LINENO: *** Automatically caching to $configure_cache ***" >&5
echo "$as_me: *** Automatically caching to $configure_cache ***" >&6;}
		>$configure_cache
		cache_file="$configure_cache"
		CONFIG_CACHE="$cache_file"
	else
		{ echo "$as_me:$LINENO: Automatic caching is unavailable on IRIX" >&5
echo "$as_me: Automatic caching is unavailable on IRIX" >&6;}
	fi

else
	echo "$as_me:$LINENO: result: $cache_file" >&5
echo "${ECHO_T}$cache_file" >&6
fi




